{
  "name": "@tanooki/storyexporter",
  "version": "0.4.36",
  "description": "exports pivotal tracker data to an sqlite database",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/tanookilabs/storyexporter",
  "bugs": "https://github.com/tanookilabs/storyexporter/issues",
  "repository": "tanookilabs/storyexporter",
  "license": "MIT",
  "author": "Peter Schilling",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "storyexporter": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && npm run copy-files",
    "copy-files": "copyfiles -u 1 src/drizzle/* src/drizzle/**/* dist/",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "storyexporter",
    "commands": "./dist/commands",
    "dirname": "storyexporter",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "better-sqlite3": "^9.4.3",
    "copyfiles": "^2.4.1",
    "drizzle-orm": "^0.30.10",
    "fetch-retry": "^6.0.0",
    "fs-extra": "^11.2.0",
    "p-ratelimit": "^1.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/better-sqlite3": "^7.6.9",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.7.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
